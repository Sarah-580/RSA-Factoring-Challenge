for line in flie:
	n = int(line.strip())
	factors = factorize(n)
	print(f"{n}={"*".join(map(str, factors))}")

if __mane__ == "__main__":
	if len(sys.argv) != 2:
		print("Usage: ./factors <file>")
		sys.exit(1)
		start_time = time.time()
		main(sys.argv[1])
		end_time = time.time()
		print(f"\nExecution time: {end_time - start_time:.6f} seconds")
